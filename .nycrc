{
  "cache": false,
  "check-coverage": false,
  "extension": [
    ".ts"
  ],
  "include": [
    "**/*.js",
    "**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts",
    "dist/esm/**",
    "dist/decimalsystem.min.js"
  ],
  "sourceMap": true,
  "reporter": [
    "text"
  ],
  "all": true,
  "instrument": true
}